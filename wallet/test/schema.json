n\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"The battery category, which can be a portable battery; starting, lighting and ignition battery (SLI); light means of transport battery (LMT); electric vehicle battery (EV), industrial battery and incorporated battery. This attribute is mentioned in the Battery regulation 2023/1542 from 12 July 2023 in Annex XIII (1) (a) and refers to ANNEX VI Part A:\\n2. the battery category and information identifying the battery in accordance with Article 38(6).\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#exampleValue\": [\n      {\n        \"@value\": \"EV\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Battery Category\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#resistanceIncrease\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Property\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#characteristic\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#ResistanceIncreaseCharacteristic\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"The increase as percentage in the battery resistance on the level of the pack, module (optional) and cell.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Resistance Increase\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#moduleResistance\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Property\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#characteristic\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#OhmMeasurement\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"The module resistance in ohms includes the internal resistance of each cell within the module as well as the resistance introduced by interconnecting components such as busbars, connectors, and thermal management systems.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#exampleValue\": [\n      {\n        \"@value\": 0.2\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Module Resistance\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#euTaxonomyDisclosureStatement\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Property\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#characteristic\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.generic.digital_product_passport:4.0.0#DocumentList\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Optional disclosure regarding the EU taxonomy, with the choice to voluntarily provide the EU Taxonomy disclosure through the battery passport.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"EU Taxonomy Disclosure Statement\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#MaterialActiveEntity\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Entity\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"The information on the active material with the recycled share, location and supporting document(s).\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Material Active Entity\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#properties\": [\n      {\n        \"@list\": [\n          {\n            \"@id\": \"urn:samm:io.catenax.generic.digital_product_passport:4.0.0#location\"\n          },\n          {\n            \"@id\": \"urn:samm:io.catenax.generic.digital_product_passport:4.0.0#recycled\"\n          },\n          {\n            \"@id\": \"urn:samm:io.catenax.generic.digital_product_passport:4.0.0#documentation\"\n          },\n          {\n            \"@id\": \"urn:samm:io.catenax.generic.digital_product_passport:4.0.0#critical\"\n          }\n        ]\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#PerformanceCharacteristic\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Characteristic\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#dataType\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#PerformanceEntity\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Performance Characteristic for the battery.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Performance Characteristic\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#activeMaterials\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Property\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#characteristic\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#ActiveMaterialsCharacteristic\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"The active materials included in the battery. Mandatory are nickel, lithium, cobalt and lead. Others are optional. Defined by the Battery regulation 2023/1542 from 12 July 2023 in Article 3 Definitions:\\n(5) \u2018active material\u2019 means a material which reacts chemically to produce electric energy when the battery cell discharges or to store electric energy when the battery is being charged.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Active Materials\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#ChemicalMaterialEntity\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Entity\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Chemical Material Entity for the battery. Included are active, hazardous and critical materials as well as a material symbol. This is mentioned in the Battery regulation 2023/1542 from 12 July 2023 in Article 13:\\n5. All batteries containing more than 0,002 % cadmium or more than 0,004 % lead, shall be marked with the chemical symbol for the metal concerned: Cd or Pb.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Chemical Material Entity\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#properties\": [\n      {\n        \"@list\": [\n          {\n            \"@id\": \"_:nb3b5b943fa62432680d490036795e45fb29\"\n          },\n          {\n            \"@id\": \"_:nb3b5b943fa62432680d490036795e45fb30\"\n          },\n          {\n            \"@id\": \"_:nb3b5b943fa62432680d490036795e45fb31\"\n          }\n        ]\n      }\n    ]\n  },\n  {\n    \"@id\": \"_:nb3b5b943fa62432680d490036795e45fb29\",\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#payloadName\": [\n      {\n        \"@value\": \"active\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#property\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#activeMaterials\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"_:nb3b5b943fa62432680d490036795e45fb30\",\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#payloadName\": [\n      {\n        \"@value\": \"hazardous\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#property\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#hazardousSubstance\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"_:nb3b5b943fa62432680d490036795e45fb31\",\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#payloadName\": [\n      {\n        \"@value\": \"composition\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#property\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#materialComposition\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#warranty\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Property\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#characteristic\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#WarrantyCharacteristic\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"The warranty of the battery. This attribute is mentioned in the Battery regulation 2023/1542 from 12 July 2023 in ANNEX XIII:\\n(m) period for which the commercial warranty for the calendar life applies.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Warranty\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#removalFromAppliance\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Property\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#characteristic\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.generic.digital_product_passport:4.0.0#DocumentList\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Removal refers to the proper process of disassembling a battery from an appliance, typically for purposes such as repairs, replacements, or disposal.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Removal From Appliance\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#powerValue\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#Property\"\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#characteristic\": [\n      {\n        \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#WattMeasurement\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#description\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Value describing the power in a specific measurement period, expressed in watt.\"\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#exampleValue\": [\n      {\n        \"@value\": 40000.0\n      }\n    ],\n    \"urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#preferredName\": [\n      {\n        \"@language\": \"en\",\n        \"@value\": \"Power Value\"\n      }\n    ]\n  },\n  {\n    \"@id\": \"urn:samm:io.catenax.battery.battery_pass:5.0.0#capabilityAt20SoC\",\n    \"@type\": [\n      \"urn:samm:org.eclipse.esmf.